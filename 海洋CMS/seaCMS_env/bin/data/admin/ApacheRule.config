RewriteBase /{cmspath}
#channel
RewriteRule {channelDirName3}/{channelpagename3}(.+?){fileSuffix}$ {channelDirName}/\?$1{fileSuffix} 
#content
RewriteRule {contentdirname3}/{contentpagename3}(.+?){fileSuffix}$ {contentDirName}/\?$1{fileSuffix}
